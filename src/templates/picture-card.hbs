{{#each this}}
<div class="photo-card">
    <img src="{{webformatURL}}" alt="picture" class="photo_card-img"/>

    <div class="stats">
        <p class="stats-item">
            <i class="material-icons md-24">thumb_up</i>
            {{likes}}
        </p>
        <p class="stats-item">
            <i class="material-icons md-24">visibility</i>
            {{views}}
        </p>
        <p class="stats-item">
            <i class="material-icons md-24">comment</i>
            {{comments}}
        </p>
        <p class="stats-item">
            <i class="material-icons md-24">cloud_download</i>
            {{downloads}}
        </p>
    </div>
</div>
{{/each}}